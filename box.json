{
  "main": "bin/application",
  "directories": [
    "src"
  ],
  "finder": [
    {
      "in": "vendor",
      "exclude": ["tests", "Tests"],
      "name": "*.php"
    },
    {
      "in": "vendor",
      "exclude": ["tests", "Tests"],
      "name": "*.json"
    }
  ],
  "output": "docker-env-generator.phar",
  "compactors": "Herrera\\Box\\Compactor\\Php",
  "compression": "GZ",
  "git-version": "package_version",
  "stub": true
}